{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["src/**"],
                "tsconfig": "tsconfig.json",
                "nodeModulesCacheMaxAge": 0
            }
        }
    ],
    "routes": [
        {
            "src": "/v1/chat/completions",
            "dest": "src/index.ts"
        },
        {
            "src": "/token/check",
            "dest": "src/index.ts"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "src/index.ts",
            "headers": {
                "Access-Control-Allow-Credentials": "true",
                "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT",
                "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Content-Type, Authorization"
            }
        }
    ],
    "functions": {
        "src/index.ts": {
            "runtime": "@vercel/node@latest",
            "maxDuration": 60,
            "memory": 1024
        }
    }
}